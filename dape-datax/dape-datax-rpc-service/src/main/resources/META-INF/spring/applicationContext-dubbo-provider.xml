<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd ">

    <dubbo:application name="dape-datax-rpc-service"/>

    <dubbo:registry address="zookeeper://zkserver:2181"/>

    <dubbo:protocol name="dubbo" port="30885"/>

    <!--<dubbo:monitor protocol="registry"/>-->

    <!-- 系统 -->
    <bean id="dataxTaskService" class="com.dape.datax.rpc.service.impl.DataxTaskServiceImpl"/>
    <dubbo:service interface="com.dape.datax.rpc.api.DataxTaskService" ref="dataxTaskService" timeout="10000"/>

    <!-- 组织 -->
    <bean id="dataxSourceTypeService" class="com.dape.datax.rpc.service.impl.DataxSourceTypeServiceImpl"/>
    <dubbo:service interface="com.dape.datax.rpc.api.DataxSourceTypeService" ref="dataxSourceTypeService" timeout="10000"/>




</beans>